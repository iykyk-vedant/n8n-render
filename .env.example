# n8n Environment Variables

# Protocol and Port
N8N_PROTOCOL=https
N8N_PORT=5678

# Environment
NODE_ENV=production

# Security
N8N_ENCRYPTION_KEY=generate_a_secure_key

# Database (if using external database)
DB_TYPE=postgresdb
DB_POSTGRESDB_HOST=postgresql://username:password@host:port/database

# Execution Process
EXECUTIONS_PROCESS=main
EXECUTIONS_MODE=regular

# Logging
N8N_LOG_LEVEL=info

# Public URL (important for OAuth callbacks)
N8N_PUBLIC_URL=https://your-render-url.onrender.com